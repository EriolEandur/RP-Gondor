{
    "multipart": [
        {   "when": {"age": "0", "north": false, "east": false, "south": false, "west": false, "up": false},
            "apply": [
                { "model": "fire_floor0" },
                { "model": "fire_floor1" }
            ]
        },
        {   "when": {"OR": [{"age": "0", "north": true}, {"age": "0", "north": false, "east": false, "south": false, "west": false, "up": false}]},
            "apply": [
                { "model": "fire_side0" },
                { "model": "fire_side1" },
                { "model": "fire_side_alt0" },
                { "model": "fire_side_alt1" }
            ]
        },
        {   "when": {"OR": [{"age": "0", "east": true}, {"age": "0", "north": false, "east": false, "south": false, "west": false, "up": false}]},
            "apply": [
                { "model": "fire_side0", "y": 90 },
                { "model": "fire_side1", "y": 90 },
                { "model": "fire_side_alt0", "y": 90 },
                { "model": "fire_side_alt1", "y": 90 }
            ]
        },
        {   "when": {"OR": [{"age": "0", "south": true}, {"age": "0", "north": false, "east": false, "south": false, "west": false, "up": false}]},
            "apply": [
                { "model": "fire_side0", "y": 180 },
                { "model": "fire_side1", "y": 180 },
                { "model": "fire_side_alt0", "y": 180 },
                { "model": "fire_side_alt1", "y": 180 }
            ]
        },
        {   "when": {"OR": [{"age": "0", "west": true}, {"age": "0", "north": false, "east": false, "south": false, "west": false, "up": false}]},
            "apply": [
                { "model": "fire_side0", "y": 270 },
                { "model": "fire_side1", "y": 270 },
                { "model": "fire_side_alt0", "y": 270 },
                { "model": "fire_side_alt1", "y": 270 }
            ]
        },
        {   "when": {"age": "0", "up": true},
            "apply": [
                { "model": "fire_up0" },
                { "model": "fire_up1" },
                { "model": "fire_up_alt0" },
                { "model": "fire_up_alt1" }
            ]
        },
        {   "when": {"age": "1"},
            "apply": { "model": "unused/unused_cross_0" }
        }
    ]
}
