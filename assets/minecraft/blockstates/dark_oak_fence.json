{
    "multipart": [
        {   "when": { "north": "false", "west": "false", "south": "false", "east": "false" },
            "apply": { "model": "stone/cobble_diagonal_pillar", "uvlock": true }
        },
        {   "when": { "north": "true", "west": "false" },
            "apply": { "model": "stone/cobble_diagonal", "uvlock": true }
        },
        {   "when": { "north": "true", "east": "false" },
            "apply": { "model": "stone/cobble_diagonal_alt", "uvlock": true }
        },
        {   "when": { "east": "true", "north": "false" },
            "apply": { "model": "stone/cobble_diagonal", "y": 90, "uvlock": true }
        },
        {   "when": { "east": "true", "south": "false" },
            "apply": { "model": "stone/cobble_diagonal_alt", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true", "east": "false" },
            "apply": { "model": "stone/cobble_diagonal", "y": 180, "uvlock": true }
        },
        {   "when": { "south": "true", "west": "false" },
            "apply": { "model": "stone/cobble_diagonal_alt", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true", "south": "false" },
            "apply": { "model": "stone/cobble_diagonal", "y": 270, "uvlock": true }
        },
        {   "when": { "west": "true", "north": "false" },
            "apply": { "model": "stone/cobble_diagonal_alt", "y": 270, "uvlock": true }
        }
    ]
}
